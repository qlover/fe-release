{
  "name": "@qlover/fe-release",
  "version": "0.0.0",
  "description": "fe release",
  "type": "module",
  "private": false,
  "exports": {
    ".": {
      "require": "./packages/lib/index.js",
      "import": "./packages/lib/index.js",
      "types": "./packages/types/index.d.ts"
    },
    "./package.json": "./package.json"
  },
  "types": "./packages/types/index.d.ts",
  "files": [
    "packages",
    "package.json",
    "commitlint.config.js",
    ".release-it.json",
    "README.md"
  ],
  "repository": {
    "type": "github",
    "url": "https://github.com/qlover/fe-release"
  },
  "homepage": "https://github.com/qlover/fe-release#readme",
  "bin": {
    "fe-release": "./packages/bin/release.js"
  },
  "scripts": {
    "build": "echo \"no build script\"",
    "eslint": "eslint packages tests --ext .js,.ts --cache --fix",
    "lint": "eslint packages tests --ext .js,.ts --cache --fix",
    "test": "ava",
    "prettier": "prettier packages tests --write",
    "reinit": "node ./packages/bin/reinit.js",
    "clean": "node ./packages/bin/clean.js",
    "setup:husky": "node packages/bin/setup-husky.js",
    "commit": "node ./packages/bin/commit.js",
    "release": "release-it",
    "update-version": "node ./packages/bin/update-version.js",
    "ts-run": "node ./packages/bin/ts-run.js",
    "clean:branch": "node ./packages/bin/clean-branch.js"
  },
  "keywords": [
    "release",
    "fe-release"
  ],
  "author": "qlover",
  "license": "ISC",
  "publishConfig": {
    "access": "public"
  },
  "devEngines": {
    "node": ">=18.19.0"
  },
  "packageManager": "yarn@1.22.22",
  "dependencies": {
    "@commitlint/cli": "^19.3.0",
    "@commitlint/config-conventional": "^19.2.2",
    "@octokit/rest": "^21.0.0",
    "@qlover/fe-node-lib": "^0.0.2",
    "@release-it/conventional-changelog": "^8.0.1",
    "chalk": "^5.3.0",
    "commander": "^11.1.0",
    "commitizen": "^4.3.0",
    "cosmiconfig": "^9.0.0",
    "cz-conventional-changelog": "^3.3.0",
    "dotenv": "^16.4.5",
    "execa": "^9.3.0",
    "husky": "^8.0.3",
    "inquirer": "^10.1.0",
    "is-ci": "^3.0.1",
    "lodash": "^4.17.21",
    "release-it": "^17.3.0",
    "rimraf": "^5.0.5",
    "semver": "^7.6.3",
    "shelljs": "^0.8.5"
  },
  "devDependencies": {
    "@types/lodash": "^4.17.6",
    "@types/shelljs": "^0.8.15",
    "@typescript-eslint/eslint-plugin": "^6.13.2",
    "@typescript-eslint/parser": "^6.13.2",
    "ava": "^6.1.3",
    "eslint": "^8.0.1",
    "eslint-config-prettier": "^9.1.0",
    "eslint-config-standard-with-typescript": "^40.0.0",
    "eslint-plugin-import": "^2.29.0",
    "eslint-plugin-jest": "^28.6.0",
    "eslint-plugin-n": "^16.3.1",
    "eslint-plugin-prettier": "^5.0.1",
    "eslint-plugin-promise": "^6.1.1",
    "eslint-plugin-unused-imports": "^3.0.0",
    "mock-stdio": "^1.0.3",
    "prettier": "^3.1.0",
    "sinon": "^18.0.0",
    "strip-ansi": "^7.1.0",
    "typescript": "*"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  }
}
